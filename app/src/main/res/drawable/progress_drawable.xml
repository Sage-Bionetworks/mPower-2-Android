<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/royal800" />
            <corners android:radius="@dimen/study_burst_progress_radius"/>
        </shape>
    </item>

    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/royal400" />
                <corners android:radius="@dimen/study_burst_progress_radius"/>
            </shape>
        </clip>
    </item>

</layer-list>